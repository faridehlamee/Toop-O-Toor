
@{
    ViewBag.Title = "ResetPassword";
}

<h2>ResetPassword</h2>
<br /><br /><br /><br /><br /><br />
@using (Html.BeginForm("ForgotPassword", "Home"))
{
    <input type="text" name="@ViewBag.UserID" />
    <input type="text" name="@ViewBag.PasswordToken" />
    <input type="submit" value="Submit" />
}
@Html.Raw(ViewBag.FakeEmailMessage)
